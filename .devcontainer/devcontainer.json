{
	"name": "Go",
	"dockerComposeFile": [
		"docker-compose.yml"
	],
	"service": "golang",
	"workspaceFolder": "..",
	"settings": {
		"go.toolsManagement.checkForUpdates": "local",
		"go.useLanguageServer": true,
		"go.gopath": "/go",
		"go.goroot": "/usr/local/go",
		"search.exclude": {
			"**/node_modules": true,
			"**/bower_components": true,
			"**/*.code-search": true,
			"**/env/**": true,
			"**/.vscode/**": true,
			"**/venv/**": true
		},
		"files.watcherExclude": {
			"**/target/**": true,
			"**/node_modules/*/**": true,
			"**/.hg/store/**": true,
			"**/.git/objects/**": true,
			"**/.git/subtree-cache/**": true,
			"**/node_modules/**": true,
			"**/env/**": true,
			"**/venv/**": true,
			"**/.vscode/**": true,
			"env-*": true
		}
	},
	"extensions": [
		"golang.Go",
		"mutantdino.resourcemonitor",
		"mechatroner.rainbow-csv",
		"donjayamanne.githistory",
		"felipecaputo.git-project-manager",
		"eamodio.gitlens",
		"oderwat.indent-rainbow",
		"ms-azuretools.vscode-docker",
		"ms-vscode.azurecli",
		"ms-azuretools.vscode-bicep",
		"wraith13.bracket-lens",
		"redhat.vscode-yaml",
		"coenraads.bracket-pair-colorizer",
		"aaron-bond.better-comments",
		"yzhang.markdown-all-in-one",
		"github.remotehub",
		"vsls-contrib.codetour"
	],
	"shutdownAction": "none",
	"postCreateCommand": "git config --global user.email 'no-reply@lyscm.github.com' && git config --global user.name 'lyscm'"
}
